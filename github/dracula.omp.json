{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .UserName }} ",
          "foreground": "#f8f8f2",
          "background": "#6272a4",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " {{ .Path }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#bd93f9",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " \ue725 ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#ffb86c",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#f8f8f2",
          "powerline_symbol": "\ue0b0",
          "background": "#8be9fd",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "trailing_diamond": "\ue0b0",
          "template": " ♥ {{ .CurrentDate | date .Format }} ",
          "foreground": "#f8f8f2",
          "background": "#ff79c6",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#282a36",
          "background": "#f1fa8c",
          "type": "aws",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "➜ ",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
